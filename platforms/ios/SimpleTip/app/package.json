{
	"name": "stackblitz-nativescript-typescript",
	"id": "org.nativescript.app",
	"appPath": "app",
	"appResourcesPath": "../../tools/assets/App_Resources",
	"android": {
		"v8Flags": "--expose_gc",
		"markingMode": "none",
		"screenOrientation": "portrait"
	},
	"ios": {
		"discardUncaughtJsExceptions": true,
		"preferredScreenOrientation": "portrait"
	},
	"main": "bundle",
	"discardUncaughtJsExceptions": true
}
